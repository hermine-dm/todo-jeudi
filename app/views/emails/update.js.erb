//alert("le fichier JS est exécuté!");
document.getElementById("content-email").innerHTML = "<%= j render 'show', email: @email %>";

if (document.getElementById("<%= @email.id%>").childNodes[1].classList.length > 1){
	document.getElementById("<%= @email.id%>").childNodes[1].classList.remove("font-weight-bold");
}
else{
	document.getElementById("<%= @email.id%>").childNodes[1].classList.add("font-weight-bold");
}
